function removeSpace(testo) {
    var str = testo;
    str = str.replace(/ +/g, "");
    return str;
}

function getTraduzione(nome, testo) {
    var result = "";
    if (jsTrad.length == 0 && getCookieLang().toLowerCase() != 'it') {
        readTraduzioni();
        jsTrad = traduzioni;

        return getLabel(nome, testo);


    }
    else {
        return getLabel(nome, testo);
    }

}
