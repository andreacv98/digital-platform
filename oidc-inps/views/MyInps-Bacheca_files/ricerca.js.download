		if (!$(divShare).hasClass("showed")) {
			$(divShare).show();
			$(divShare).addClass("showed");

			var itemList = document.getElementsByClassName("showed");

			for (var i = 0; i < itemList.length; i++) {
				if (itemList[i].id != divShare.id) {
					$(itemList[i]).hide();
					$(itemList[i]).removeClass("showed");
				}
			}
		}
	}			
